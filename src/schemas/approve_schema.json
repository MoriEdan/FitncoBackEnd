{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Approve",
  "description": "Approve",
  "type": "object",
  "properties": {
    "approve": {
      "type": "integer",
      "enum": [
        0,
        1
      ]
    },
    "dietician": {
      "type": ["string", "integer"],
      "enum": [
        "0",
        0,
        "1",
        1
      ]
    },
    "gender": {
      "type": "string",
      "enum": [
        "male",
        "female"
      ]
    },
    "type": {
      "type": "string",
      "enum": [
        "facetoface",
        "online"
      ]
    },
    "age": {
      "type": [
        "integer",
        "string"
      ]
    },
    "weight": {
      "type": [
        "number",
        "string"
      ]
    },
    "height": {
      "type": [
        "number",
        "string"
      ]
    },
    "target": {
      "type": [
        "number",
        "string"
      ]
    },
    "notes": {
      "type": [
        "null",
        "string"
      ]
    },
    "vip": {
      "type": [
        "boolean",
        "string"
      ]
    },
    "user_id": {
      "type": [
        "integer",
        "string"
      ]
    },
    "name": {
      "type": "string",
      "minLength": 2,
      "maxLength": 255
    },
    "last_name": {
      "type": "string",
      "minLength": 2,
      "maxLength": 255
    },
    "can_walk": {
      "type": ["string", "boolean"],
      "enum": [
        "0",
        "1",
        true,
        false
      ]
    }
  },
  "required": [
    "approve",
    "user_id",
    "name",
    "last_name"
  ]
}